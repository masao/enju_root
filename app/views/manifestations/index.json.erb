[
<% @manifestations.each_with_index{|manifestation, i| %>
{
  "@id": "<%= manifestation_url(manifestation) %>",
  "@type": "http://purl.org/vocab/frbr/core#Manifestation",
  "http://purl.org/dc/terms/title": "<%= manifestation.title_proper %>",
  "http://purl.org/dc/terms/identifier": "<%= manifestation.url %>",
  "http://purl.org/vocab/frbr/core#embodimentOf":
    <%=raw manifestation.expressions.map{|expression| work_url(expression.work)}.to_s %>
    }
    <% if @manifestations.size > i + 1 %>,<% end %>
<% } %>
]
